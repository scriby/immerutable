{
  "name": "immerutable",
  "version": "0.0.1",
  "devDependencies": {
    "@types/jest": "22.2.3",
    "immer": "1.2.1",
    "jest": "22.4.3",
    "ts-jest": "^22.4.4",
    "typescript": "2.8.3"
  },
  "scripts": {
    "benchmark": "node --expose-gc ./node_modules/.bin/jest benchmark --verbose",
    "benchmark-debug": "node --inspect-brk --expose-gc ./node_modules/.bin/jest benchmark --verbose",
    "build": "tsc",
    "test": "jest src --watch",
    "test-debug": "node --inspect-brk ./node_modules/.bin/jest src --watch"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "((\\.|/)test)\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  }
}
